{
	"name": "Where Have I Been",
	"version": "0.1",
	"description": "Track your Google Chrome history in a Google Sheet",
	"permissions": [
		"declarativeContent",
		"storage",
		"activeTab",
		"history",
		"identity",
		"https://filipkin.com/whib/*"
	],
	"background": {
		"scripts": [
			"install.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup/index.html",
		"default_icon": {
			"16": "img/logo-16.png",
			"32": "img/logo-32.png",
			"48": "img/logo-48.png",
			"64": "img/logo-64.png",
			"128": "img/logo-128.png"
		}
	},
	"icons": {
		"16": "img/logo-16.png",
		"32": "img/logo-32.png",
		"48": "img/logo-48.png",
		"64": "img/logo-64.png",
		"128": "img/logo-128.png"
	},
	"manifest_version": 2
}
